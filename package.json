{
  "name": "preact-limeapp",
  "version": "0.0.0",
  "description": "Limeapp webUI and app",
  "scripts": {
    "dev": "cross-env NODE_ENV=development webpack-dev-server --inline --hot --progress",
    "start": "serve build -s -c 1",
    "prestart": "npm run build",
    "build": "cross-env NODE_ENV=production webpack -p --progress --display-optimization-bailout --optimize-minimize && rm ./build/*.map",
    "prebuild": "mkdirp build && ncp src/assets build/assets",
    "deploy": "gh-pages -d build",
    "test": "npm run -s lint && npm run -s test:karma",
    "test:karma": "karma start test/karma.conf.js --single-run",
    "lint": "eslint src",
    "translations": "node ./node_modules/i18nline/bin/i18nline.js export"
  },
  "keywords": [
    "preact",
    "boilerplate",
    "webpack"
  ],
  "license": "GNU",
  "author": "Marcos Gutierrez <gmarcos@altermundi.net>",
  "devDependencies": {
    "autoprefixer": "^7.1.2",
    "babel": "^6.23.0",
    "babel-core": "^6.25.0",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.1.1",
    "babel-plugin-module-resolver": "^2.7.1",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-transform-async-to-generator": "^6.24.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-react-jsx": "^6.24.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.24.1",
    "babel-runtime": "^6.25.0",
    "chai": "^4.1.0",
    "copy-webpack-plugin": "^4.0.1",
    "core-js": "^2.4.1",
    "cross-env": "^5.0.1",
    "css-loader": "^0.28.4",
    "eslint": "^4.3.0",
    "extract-text-webpack-plugin": "^3.0.0",
    "file-loader": "^0.11.2",
    "gh-pages": "^1.0.0",
    "html-webpack-plugin": "^2.30.1",
    "i18nline": "^1.5.0",
    "isparta-loader": "^2.0.0",
    "json-loader": "^0.5.7",
    "karma": "^1.7.0",
    "karma-chai": "^0.1.0",
    "karma-chai-sinon": "^0.1.5",
    "karma-coverage": "^1.1.1",
    "karma-mocha": "^1.3.0",
    "karma-mocha-reporter": "^2.2.3",
    "karma-phantomjs-launcher": "^1.0.4",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^2.0.4",
    "less": "^2.7.2",
    "less-loader": "^4.0.5",
    "mkdirp": "^0.5.1",
    "mocha": "^3.5.0",
    "ncp": "^2.0.0",
    "offline-plugin": "^4.8.3",
    "postcss-loader": "^2.0.6",
    "raw-loader": "^0.5.1",
    "replace-bundle-webpack-plugin": "^1.0.0",
    "script-ext-html-webpack-plugin": "^1.8.5",
    "sinon": "^3.0.0",
    "sinon-chai": "^2.12.0",
    "source-map-loader": "^0.2.1",
    "style-loader": "^0.18.2",
    "url-loader": "^0.5.9",
    "webpack": "^3.4.1",
    "webpack-dev-server": "^2.6.1"
  },
  "dependencies": {
    "@libremesh/limeapp-plugin-locate": "^0.0.4",
    "history": "^4.6.3",
    "leaflet": "^1.2.0",
    "leaflet.gridlayer.googlemutant": "^0.6.2",
    "preact": "^8.2.5",
    "preact-compat": "^3.16.0",
    "preact-i18nline": "^1.5.0",
    "preact-redux": "^2.0.2",
    "preact-router": "^2.5.5",
    "preact-router-redux": "^4.1.0",
    "promise-polyfill": "^6.0.2",
    "proptypes": "^1.1.0",
    "purecss": "^1.0.0",
    "react-load-script": "0.0.6",
    "react-pure": "^0.3.1",
    "redux": "^3.7.2",
    "redux-observable": "^0.15.0",
    "redux-thunk": "^2.2.0",
    "rxjs": "^5.4.2",
    "serve": "^6.0.4",
    "simple-color-scale": "^1.0.1",
    "simplegrid": "^1.0.2",
    "skeleton-less": "^2.0.1",
    "ulog": "^1.0.3"
  }
}
