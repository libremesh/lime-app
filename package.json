{
  "private": true,
  "name": "preact-limeapp",
  "version": "0.2.0-alpha.13",
  "description": "Limeapp webUI and app",
  "license": "GNU",
  "author": "Marcos Gutierrez <gmarcos@altermundi.net>",
  "scripts": {
    "start": "if-env NODE_ENV=production && npm run -s serve || npm run -s dev",
    "build": "preact build --no-prerender && rm build/*.map",
    "serve": "preact build && preact serve",
    "dev": "preact watch",
    "lint": "eslint src",
    "deploy": "gh-pages -d build",
    "translations": "node ./node_modules/i18nline/bin/i18nline.js export",
    "posttranslations": "node ./i18n-diff.js",
    "cz": "git-cz",
    "release": "standard-version"
  },
  "eslintConfig": {
    "extends": "eslint-config-synacor"
  },
  "eslintIgnore": [
    "build/*"
  ],
  "devDependencies": {
    "@commitlint/cli": "^8.0.0",
    "@commitlint/config-conventional": "^8.0.0",
    "commitizen": "^4.0.0",
    "commitlint": "^8.0.0",
    "dotenv": "^8.0.0",
    "eslint": "^6.0.0",
    "eslint-config-synacor": "^3.0.2",
    "gh-pages": "^2.0.1",
    "husky": "^3.0.0",
    "if-env": "^1.0.0",
    "less": "^3.0.4",
    "less-loader": "^5.0.0",
    "preact-cli": "^2.0.0",
    "standard-version": "^7.0.0"
  },
  "dependencies": {
    "axios": "^0.19.0",
    "history": "^4.7.2",
    "i18n-js": "^3.0.3",
    "jssha": "^2.3.1",
    "leaflet": "^1.3.1",
    "leaflet.gridlayer.googlemutant": "^0.8.0",
    "preact": "^10.0.0",
    "preact-compat": "^3.17.0",
    "preact-i18nline": "^2.0.0",
    "preact-redux": "^2.0.3",
    "preact-router": "^2.6.0",
    "preact-router-redux": "^4.1.0",
    "react-load-script": "0.0.6",
    "redux": "^4.0.0",
    "redux-observable": "^1.0.0-beta.2",
    "redux-thunk": "^2.2.0",
    "rxjs": "^6.1.0",
    "rxjs-compat": "^6.1.0",
    "simple-color-scale": "^1.0.1",
    "simplegrid": "^1.0.2",
    "skeleton-less": "^2.0.1"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -e $HUSKY_GIT_PARAMS"
    }
  }
}
