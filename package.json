    "redux": "^3.6.0",
    "redux-observable": "^0.14.1",
    "redux-thunk": "^2.2.0",
    "rxjs": "^5.2.0",
    "serve": "^3.1.0",
    "simplegrid": "^1.0.2",
    "skeleton-less": "^2.0.1",
    "ulog": "^1.0.2"
  }
