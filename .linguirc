{
   "locales": ["es", "pt", "en", "it"],
   "catalogs": [{
      "path": "i18n/{locale}/messages",
      "include": ["src", "plugins"]
   }],
   "format": "po",
   "compileNamespace": "ts",
   "extractBabelOptions": {
       "presets": [
         "preact",
         "@babel/preset-typescript",
         "@lingui/babel-preset-react"
      ]
   },
   "sourceLocale": "en",
   "fallbackLocales": {
      "default": "en"
   }
}
